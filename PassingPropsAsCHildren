//App.jsx
import { useState } from 'react'
import Button from './components/Button'
import './App.css'

function App() {
  const [count, setCount] = useState(0);
  function handleClick(){
     setCount(count+1);
  }
  return (
  <div>
    <Button handleClick={handleClick} text={`count is ${count}`} >
     <h1>{count}</h1>
    </Button>
  </div>
  )
}

export default App
